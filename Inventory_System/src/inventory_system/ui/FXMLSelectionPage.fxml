<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="inventory_system.controllers.FXMLSelectionPageController">
   <children>
      <Button fx:id="btnOffline" layoutX="90.0" layoutY="188.0" mnemonicParsing="false" onAction="#offlineInventoryScene" text="Offline">
         <font>
            <Font name="Comic Sans MS" size="12.0" />
         </font></Button>
      <Button fx:id="btnOnline" layoutX="459.0" layoutY="188.0" mnemonicParsing="false" onAction="#onlineInventoryScene" text="Online">
         <font>
            <Font name="Comic Sans MS" size="12.0" />
         </font></Button>
      <Label layoutX="90.0" layoutY="55.0" text="Please Choose Your System">
         <font>
            <Font name="Comic Sans MS" size="33.0" />
         </font>
      </Label>
      <Button fx:id="btnBack" layoutX="44.0" layoutY="348.0" mnemonicParsing="false" onAction="#backToMenu" text="Back to Menu">
         <font>
            <Font name="Comic Sans MS" size="12.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
