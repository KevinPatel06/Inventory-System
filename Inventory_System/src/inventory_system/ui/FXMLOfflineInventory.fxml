<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="inventory_system.controllers.FXMLOfflineInventoryController">
   <children>
      <Label layoutX="216.0" layoutY="32.0" text="Offline Inventory">
         <font>
            <Font name="Comic Sans MS Bold Italic" size="20.0" />
         </font>
      </Label>
      <Label fx:id="fileNameLabel" layoutX="109.0" layoutY="202.0" text="...">
         <font>
            <Font name="Comic Sans MS" size="16.0" />
         </font>
      </Label>
      <Button fx:id="fileSelectbtn" layoutX="53.0" layoutY="166.0" mnemonicParsing="false" onAction="#fileSelect" text="Select first Inventory" />
      <Button fx:id="btnBack" layoutX="44.0" layoutY="348.0" mnemonicParsing="false" onAction="#backToMenu" text="Back to Menu">
         <font>
            <Font name="Comic Sans MS" size="12.0" />
         </font>
      </Button>
      <Button fx:id="fileSelectbtn2" layoutX="349.0" layoutY="166.0" mnemonicParsing="false" onAction="#fileSelectTwo" text="Select second Inventory" />
      <Label fx:id="lblFileNameTwo" layoutX="416.0" layoutY="205.0" text="..." />
      <Button fx:id="compareBtn" layoutX="223.0" layoutY="256.0" mnemonicParsing="false" onAction="#compare" text="Compare both inventories" />
   </children>
</AnchorPane>
