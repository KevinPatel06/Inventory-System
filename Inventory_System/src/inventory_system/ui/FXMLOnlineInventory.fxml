<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="inventory_system.controllers.FXMLOnlineInventoryController">
   <children>
      <Button fx:id="btnReadTags" layoutX="135.0" layoutY="280.0" mnemonicParsing="false" onAction="#readTags" text="Read Tags">
         <font>
            <Font name="Comic Sans MS" size="15.0" />
         </font>
      </Button>
      <Button fx:id="btnBack" layoutX="44.0" layoutY="348.0" mnemonicParsing="false" onAction="#backToMenu" text="Back to Menu">
         <font>
            <Font name="Comic Sans MS" size="12.0" />
         </font>
      </Button>
      <ImageView fx:id="imageWindow" fitHeight="214.0" fitWidth="308.0" layoutX="238.0" layoutY="37.0" pickOnBounds="true" preserveRatio="true" />
   </children>
</AnchorPane>
